<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="CustomerDAO" class="com.omega.smartqueue.daos.implementations.jdbc.JDBCCustomerDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="RestaurantDAO" class="com.omega.smartqueue.daos.implementations.jdbc.JDBCRestaurantDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	<bean id="QueuesDAO" class="com.omega.smartqueue.daos.implementations.jdbc.JDBCQueuesDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
 
</beans>